<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox fx:id="mainLayout" minHeight="250.0" minWidth="250.0" spacing="5.0" stylesheets="@../resource/customStyle.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="TFG_project.VIEWS.SetUpController">
   <children>
      <HBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="40.0" minWidth="-Infinity" prefHeight="40.0" VBox.vgrow="NEVER">
         <children>
            <Label style="-fx-font-family: arialBlack;" styleClass="title1" text="Set Sessions Info">
            <HBox.margin>
               <Insets left="15.0" top="15.0" />
            </HBox.margin>
            <font>
               <Font name="Arial Black" size="14.0" />
            </font>
            </Label>
         </children>
      </HBox>
      <GridPane fx:id="sessionsGridPane" alignment="CENTER_LEFT" gridLinesVisible="true" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
        <columnConstraints>
          <ColumnConstraints halignment="CENTER" hgrow="NEVER" maxWidth="100.0" minWidth="100.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints maxHeight="40.0" minHeight="40.0" prefHeight="40.0" valignment="CENTER" vgrow="NEVER" />
            <RowConstraints maxHeight="40.0" minHeight="40.0" prefHeight="40.0" valignment="CENTER" vgrow="NEVER" />
          <RowConstraints maxHeight="40.0" minHeight="40.0" prefHeight="40.0" valignment="CENTER" vgrow="NEVER" />
          <RowConstraints maxHeight="40.0" minHeight="40.0" prefHeight="40.0" valignment="CENTER" vgrow="NEVER" />
        </rowConstraints>
         <children>
            <Label text="Start Hour" GridPane.rowIndex="2">
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
               <GridPane.margin>
                  <Insets />
               </GridPane.margin>
            </Label>
            <Label text="End Hour" GridPane.rowIndex="3">
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
               <GridPane.margin>
                  <Insets />
               </GridPane.margin>
            </Label>
            <Label text="Date" GridPane.rowIndex="1" />
         </children>
         <padding>
            <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
         </padding>
      </GridPane>
      <HBox alignment="CENTER_LEFT" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity" spacing="8.0" VBox.vgrow="NEVER">
         <children>
            <Label maxHeight="30.0" maxWidth="118.0" minHeight="-Infinity" minWidth="-Infinity" prefHeight="30.0" prefWidth="0.0" text="Add new table config" />
            <TextField fx:id="newTableValue" alignment="CENTER_RIGHT" disable="true" maxWidth="58.0" prefHeight="27.0" prefWidth="0.0" visible="false" HBox.hgrow="NEVER" />
            <Button disable="true" maxWidth="66.0" minWidth="0.0" mnemonicParsing="false" onAction="#setNewTableValue" prefHeight="25.0" prefWidth="0.0" styleClass="custom_button" text="Add" visible="false" />
            <Label maxWidth="187.0" minWidth="0.0" prefHeight="15.0" prefWidth="0.0" text="Use * to add a custom table">
               <font>
                  <Font size="11.0" />
               </font>
            </Label>
            <Pane HBox.hgrow="ALWAYS" />
            <Button mnemonicParsing="false" onAction="#saveInfoAction" prefHeight="28.0" prefWidth="68.0" styleClass="custom_button" text="Save">
               <HBox.margin>
                  <Insets right="10.0" />
               </HBox.margin></Button>
         </children>
         <padding>
            <Insets bottom="4.0" left="8.0" right="8.0" top="4.0" />
         </padding>
      </HBox>
   </children>
</VBox>
