<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox fx:id="mainVBox" style="-fx-background-color: lightGrey;" stylesheets="@customStyle.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="TFG_project.CONTROLLERS.MeetingScheduledDetailController">
    <children>
        <Label maxHeight="-Infinity" minHeight="-Infinity" prefHeight="20.0" style="-fx-font-family: arialBlack;" styleClass="title1" text="Meeting Detail">
            <padding>
                <Insets left="10.0" />
            </padding>
            <VBox.margin>
                <Insets bottom="10.0" top="10.0" />
            </VBox.margin>
            <font>
                <Font name="Arial Black" size="14.0" />
            </font>
        </Label>
      <HBox fillHeight="false" spacing="15.0" VBox.vgrow="ALWAYS">
         <children>
            <GridPane prefHeight="100.0" prefWidth="192.0">
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="121.0" minWidth="10.0" prefWidth="107.0" />
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="70.0" minWidth="10.0" prefWidth="43.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <Label text="Curent Session:" />
                  <Label fx:id="currentSessionLabel" text="Label" GridPane.columnIndex="1" />
                  <Label text="Hour:" GridPane.rowIndex="1" />
                  <Label text="Table:" GridPane.rowIndex="2" />
                  <Label fx:id="hourLabel" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                  <Label fx:id="tableLabel" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="2" />
               </children>
            </GridPane>
            <VBox fx:id="participantsVBox" spacing="10.0">
               <children>
                  <Label styleClass="title2" text="Participants:" />
               </children>
               <padding>
                  <Insets bottom="5.0" left="5.0" right="15.0" top="5.0" />
               </padding>
            </VBox>
         </children>
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </VBox.margin>
      </HBox>
    </children>
</VBox>
