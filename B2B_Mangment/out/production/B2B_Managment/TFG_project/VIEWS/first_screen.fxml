<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" spacing="15.0" style="-fx-background-color: lightgrey;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="TFG_project.VIEWS.FirstSceenController">
   <children>
      <Button fx:id="buttonCreate" blendMode="DARKEN" mnemonicParsing="false" onAction="#buttonCreateNew" style="-fx-background-color: transparent;" text="Create New Schedule" textFill="#002454">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Button blendMode="DARKEN" mnemonicParsing="false" style="-fx-background-color: transparent;" text="Open Existing Schedule" textFill="#002454">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Button blendMode="DARKEN" mnemonicParsing="false" style="-fx-background-color: transparent;" text="Create Template csv" textFill="#002454">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
   </children>
   <padding>
      <Insets bottom="40.0" left="40.0" right="40.0" top="40.0" />
   </padding>
</VBox>
